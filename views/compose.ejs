<%-include("partials/header")-%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="exampleInputEmail1">Title</label>
    <input name="postTitle" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    <label for="exampleFormControlTextarea1" class="form-label">Post</label>
    <textarea name="postBody" class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Publish</button>
</form>

<%-include("partials/footer")-%>
